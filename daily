#!/bin/sh
brew update
brew upgrade
brew cleanup -s --force
npm update -g
for x in $(cat ~/Documents/install/gems.txt ); do gem update $x; done
update-repos ~/src/* ~/projects/*
cd ~/projects/dotfiles && git submodule foreach git pull 
